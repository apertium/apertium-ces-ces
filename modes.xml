<?xml version="1.0" encoding="UTF-8"?>
<modes>
  <mode name="cs-ces_han">
    <pipeline>
      <program name="lt-proc">
        <file name="cs-ces_han.automorf.bin"/>
      </program>
      <program name="apertium-tagger -g $2">
        <file name="cs-ces_han.prob"/>
      </program>
      <program name="apertium-pretransfer"/>
      <program name="apertium-transfer">
        <file name="apertium-cs-ces_han.cs-ces_han.t1x"/>
        <file name="cs-ces_han.t1x.bin"/>
        <file name="cs-ces_han.autobil.bin"/>
      </program>
      <program name="apertium-interchunk">
        <file name="apertium-cs-ces_han.cs-ces_han.t2x"/>
        <file name="cs-ces_han.t2x.bin"/>
      </program>
      <program name="apertium-postchunk">
        <file name="apertium-cs-ces_han.cs-ces_han.t3x"/>
        <file name="cs-ces_han.t3x.bin"/>
      </program>
      <program name="lt-proc $1">
        <file name="cs-ces_han.autogen.bin"/>
      </program>
      <program name="lt-proc -p">
        <file name="cs-ces_han.autopgen.bin"/>
      </program>
    </pipeline>
  </mode>

</modes>
